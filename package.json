{
  "name": "agilizai-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo do sistema AgilizAI - Comanda digital",
  "scripts": {
    "dev": "turbo dev --parallel",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "backend:dev": "turbo dev --filter=@agilizai/backend",
    "admin:dev": "turbo dev --filter=@agilizai/admin",
    "order:dev": "turbo dev --filter=@agilizai/order"
  },
  "workspaces": [
    "backend",
    "agilizai-admin",
    "agilizai-order"
  ],
  "devDependencies": {
    "turbo": "^2.0.0"
  },
  "packageManager": "npm@11.6.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "qrcode.react": "^4.2.0"
  }
}
